# Environment Variables for Document POD
# Local Development Configuration

# Application Configuration
REACT_APP_NAME=Document POD
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Modern Document Management System

# API Configuration
REACT_APP_API_URL=https://documentpod-backend.onrender.com
REACT_APP_API_VERSION=v1

# Build Configuration
GENERATE_SOURCEMAP=false

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=true

# Cache Configuration
REACT_APP_CACHE_TTL=300000
REACT_APP_USER_CACHE_TTL=600000
REACT_APP_DOCUMENT_CACHE_TTL=180000